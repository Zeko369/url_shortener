datasource postgresql {
  provider = "postgresql"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

model Link {
  id        Int      @default(autoincrement()) @id
  url       String
  slug      String   @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  clicks    Click[]
}

model Click {
  id        Int      @default(autoincrement()) @id
  ua        String
  Link      Link?    @relation(fields: [linkId], references: [id])
  linkId    Int?
  createdAt DateTime @default(now())
}
